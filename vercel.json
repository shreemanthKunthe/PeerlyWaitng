{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "pnpm install --frozen-lockfile && pnpm build",
  "outputDirectory": "dist/spa",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/[...all]" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
